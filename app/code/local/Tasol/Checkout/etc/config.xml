<?xml version="1.0" ?>
<config>
	<modules>
		<Tasol_Checkout>
			<version>0.0.1</version>
		</Tasol_Checkout>
	</modules>	
	<frontend>
		<routers>
			<checkout>
                <args>
                    <modules>
                        <Tasol_Checkout before="Mage_Checkout">Tasol_Checkout</Tasol_Checkout>
                    </modules>
                </args>
            </checkout>
		</routers>
	</frontend>
	<global>
		<helpers>
            <personalize>
                <class>Tasol_Checkout_Helper</class>
            </personalize>
        </helpers>
    </global>
</config>